{
  "name": "@laurenzrausche/env",
  "version": "0.0.0",
  "description": "A simple process.env tool for my projects.",
  "keywords": [
    "env",
    "process.env",
    "process"
  ],
  "homepage": "https://github.com/laurenzrausche/env#readme",
  "bugs": {
    "url": "https://github.com/laurenzrausche/env/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/laurenzrausche/env.git"
  },
  "license": "MIT",
  "author": "Laurenz Rausche <laurenz@laurenz-rausche.de>",
  "private": false,
  "main": "dist/index.js",
  "scripts": {
    "build": "npm run console:clear && npm run build:clean && npm run build:compile",
    "build:clean": "rimraf dist",
    "build:compile": "tsc && tsc-alias",
    "console:clear": "node -e \"console.clear()\"",
    "prepare": "npm run build",
    "test": "rimraf test/*.log && npm run test:with-envs && npm run test:without-envs",
    "test:with-envs": "_0_UNSET=SET-0 _1_REQUIRED=SET-1 _2_NOT_REQUIRED=SET-2 _3_FALLBACK=SET-3 _4_NO_FALLBACK=SET-4 _5_REQUIRED_FALLBACK=SET-5 _6_REQUIRED_NO_FALLBACK=SET-6 _7_NOT_REQUIRED_FALLBACK=SET-7 _8_NOT_REQUIRED_NO_FALLBACK=SET-8 tsx test/test.ts",
    "test:without-envs": "tsx test/test.ts"
  },
  "publishConfig": {
    "access": "public"
  },
  "devDependencies": {
    "@laurenzrausche/env": "file:./",
    "@types/node": "^25.0.3",
    "rimraf": "^6.1.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3"
  }
}
